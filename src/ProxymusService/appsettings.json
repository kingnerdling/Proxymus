{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Proxy": {
    "Name": "Proxy1",
    "MessageQueueLength": 2000,
    "Frontend": {
      "Type": "ProxymusCore.Frontend.SocketFrontend",
      "Configuration": {
        "Name": "Frontend",
        "IpAddress": "*",
        "Port": 6000,
        "MaxMessageCount": 1,
        "IdleTimeout": 30000,
        "MaxClientCount": 10,
        "MaxClientQueueCount": 100,
        "BufferSize": 4096,
        "MessageProcessor": "ProxymusCore.MessageProcessor.HeaderLengthMessageProcessor"
      }
    },
    "Backend": {
      "Name": "Backend",
      "Type": "ProxymusCore.MessageProcessor.PersistentSocketBackend",
      "Router": {
        "Type": "ProxymusCore.Router.RoundRobinRouter"
      },
      "HostConfigurations": [
        {
          "Name": "Host1",
          "IpAddress": "127.0.0.1",
          "Port": 6001,
          "MaxMessageCount": 10,
          "IdleTimeoutMs": 30000,
          "ClientCount": 32,
          "BufferSize": 4096,
          "ReconnectIntervalMs": 5000,
          "ReceiveTimeoutMs": 5000,
          "MessageProcessor": "ProxymusCore.MessageProcessor.HeaderLengthMessageProcessor"
        },
        {
          "Name": "Host2",
          "IpAddress": "127.0.0.1",
          "Port": 6001,
          "MaxMessageCount": 10,
          "IdleTimeoutMs": 30000,
          "ClientCount": 32,
          "BufferSize": 4096,
          "ReconnectIntervalMs": 5000,
          "ReceiveTimeoutMs": 5000,
          "MessageProcessor": "ProxymusCore.MessageProcessor.HeaderLengthMessageProcessor"
        }
      ]
    }
  }
}
